{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.4.0",
    "generators": {
      "api_reference": {
        "generatorName": "markdown",
        "glob": "./src/.vuepress/public/api/production/definitions/open-api/apiDefinition.swagger.yaml",
        "output": "#{cwd}/src/api/production/reference"
      },
      "javascript_sdk": {
        "generatorName": "javascript",
        "glob": "./src/.vuepress/public/api/production/definitions/open-api/apiDefinition.swagger.yaml",
        "output": "#{cwd}/src/api/production/sdks/javascript"
      }
    }
  }
}
